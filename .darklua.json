{
  "rules": [
    "remove_nil_declaration",
    "remove_empty_do",
    "remove_unused_if_branch",
    "remove_unused_while",
    "compute_expression",
    "filter_after_early_return",
    {
      "rule": "convert_require",
      "current": {
        "name": "path",
        "sources": {
          "@pkg": "roblox_packages",
          "@net": "src/network",
          "@shared": "src/shared"
        }
      },
      "target": {
        "name": "roblox",
        "rojo_sourcemap": "sourcemap.json",
        "indexing_style": "property"
      }
    }
  ]
}
